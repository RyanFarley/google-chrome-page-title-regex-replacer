{
  "name": "Title Regex Replacer",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Allows for site titles to be changed with regular expressions",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://docs.djangoproject.com/*"],
      "js": ["title_replace.js"],
      "run_at": "document_end"
    }
  ]
}